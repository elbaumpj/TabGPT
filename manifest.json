{
  "manifest_version": 3,
  "name": "TabGPT",
  "description": "Go to ChatGPT when a new tab opens",
  "version": "1.0",
  "action": {
    "default_icon": "images/icon-16.png"
  },
  "permissions": [
    "tabs"
  ],
   "background": {
    "service_worker": "background.js"
  },
  "icons": {
      "16": "images/icon-16.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
}